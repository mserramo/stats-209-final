y_logit = glm(outcome ~ receive + assign + sex + race + age + copd + dm + heartd + renal + liverd,
data=df,
family=binomial)
log_or <- summary(y_logit)$coef[2, 1:2]
est = exp(log_or[1])
est
get_e <- function(RR) {
RR + sqrt(RR * (RR-1))
}
get_e(est)
get_e(1/est)
